{
  "name": "eurosport",
  "version": "1.0.0",
  "description": "An example React, GraphQL, Next and Apollo",
  "main": "index.js",
  "scripts": {
    "dev": "next -p 3000",
    "build": "next build",
    "start": "next start",
    "deploy": "next build && next start",
    "test": "NODE_ENV=test jest --watch",
    "test-win": "SET NODE_ENV=test&& jest --watch",
    "heroku-postbuild": "next build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apollo-boost": "^0.4.4",
    "apollo-client": "^2.6.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-styled-components": "^1.10.6",
    "date-fns": "^2.0.1",
    "graphql": "^14.5.4",
    "graphql-tag": "^2.10.1",
    "lodash.debounce": "^4.0.8",
    "next": "^9.3.2",
    "next-with-apollo": "^4.2.0",
    "nprogress": "^0.2.0",
    "react": "^16.9.0",
    "react-apollo": "^2.5.8",
    "react-dom": "^16.9.0",
    "react-transition-group": "^2.5.0",
    "styled-components": "^4.3.2",
    "waait": "^1.0.5"
  },
  "devDependencies": {
    "babel-plugin-module-resolver": "^3.2.0",
    "graphql-tools": "^4.0.5"
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "test": {
        "presets": [
          [
            "next/babel",
            {
              "preset-env": {
                "modules": "commonjs"
              }
            }
          ]
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      }
    }
  }
}
